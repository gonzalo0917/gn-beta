<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>
<section id="brand">
  <h1 class="icon logo left">goninis | Beta privado</h1>
  <div class="clickable icon ingresa right">
    <a href="#">Ingresa</a>
  </div>
</section>
<section id="categories">
<% @categories.each do |category| %>
  <div class="icon category <%= cycle("left", "right") -%>">
    <%= image_tag(category.icon) %> 
    <h3><%= category.title %></h3>
  </div>
<% end %>
</section>